{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "typedb-web": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "td",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/favicon.png",
              "src/assets",
              "src/robots.txt",
              "src/site.webmanifest",
              "src/sitemap.xml",
              {
                "glob": "**/*",
                "input": "./node_modules/typedb-web-common/src/icons",
                "output": "/assets/icon/"
              }
            ],
            "styles": [
              "./node_modules/cookieconsent/build/cookieconsent.min.css",
              "src/styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "./node_modules/typedb-web-common/src/styles"
              ]
            },
            "scripts": [
              "./node_modules/cookieconsent/build/cookieconsent.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "server": "src/main.server.ts",
              "outputMode": "server",
              "ssr": {
                "entry": "server.ts"
              }
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environment/environment.ts",
                  "with": "src/environment/environment.dev.ts"
                }
              ],
              "server": "src/main.server.ts",
              "outputMode": "server",
              "ssr": {
                "entry": "server.ts"
              }
            },
            "no-ssr": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environment/environment.ts",
                  "with": "src/environment/environment.dev.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "typedb-web:build:production"
            },
            "development": {
              "buildTarget": "typedb-web:build:development"
            },
            "no-ssr": {
              "buildTarget": "typedb-web:build:no-ssr"
            }
          },
          "defaultConfiguration": "no-ssr"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.png",
              "src/assets",
              "src/robots.txt"
            ],
            "styles": [
              "src/styles/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
    ]
  }
}
